{
  "compilerOptions": {
    "target": "es3",
    "noImplicitAny": false,
    "module": "amd",
    "removeComments": true,
    "sourceMap": true,
    "baseUrl": "modules",
    "paths": {
      "SeedModules.AngularUI/*": ["SeedModules.AngularUI/wwwroot/*"],
      "SeedModules.Setup/*": ["SeedModules.Setup/wwwroot/*"],
      "SeedModules.Admin/*": ["SeedModules.Admin/wwwroot/*"],
      "SeedModules.Features/*": ["SeedModules.Features/wwwroot/*"],
      "SeedModules.MindPlus/*": ["SeedModules.MindPlus/wwwroot/*"],
      "SeedModules.PageBuilder/*": ["SeedModules.PageBuilder/wwwroot/*"],
      "SeedModules.Saas/*": ["SeedModules.Saas/wwwroot/*"],
      "SeedModules.SqlBuilder/*": ["SeedModules.SqlBuilder/wwwroot/*"]
    }
  },
  "exclude": ["node_modules", "src", "targets"]
}
