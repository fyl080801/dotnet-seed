{
  "compilerOptions": {
    "target": "es3",
    "noImplicitAny": false,
    "module": "amd",
    "removeComments": true,
    "sourceMap": true,
    "baseUrl": "modules",
    "paths": {
      "SeedModules.AngularUI/*": ["SeedModules.AngularUI/Content/*"],
      "SeedModules.Admin/*": ["SeedModules.Admin/Content/*"],
      "SeedModules.Features/*": ["SeedModules.Features/Content/*"],
      "SeedModules.MindPlus/*": ["SeedModules.MindPlus/Content/*"]
    }
  },
  "exclude": ["node_modules", "src", "targets"]
}
